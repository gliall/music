cps 0.2

d1 $ density 1 $  sound "morgan:1"
d2 $ density 0.2 $ striate 12 $ sound "morgan:2"
d3 $ striate 128  $ sound "morgan:1"

d1 $ silence
d2 $ silence
d3 $ silence



d1 $ chop 2 $ sound (every 3 (density 0.5) "{bd [ht sn, lt mt ht] lt, sn cp}%6")
   |+| vowel "a o e"
   |+| pan "0 0.5 1"



d1 $stut 4 0.5 0.2 $ sound "[bd bd] [bd [sn [sn sn] sn] sn]"
   |+| pan sinewave1
   |+| shape sinewave1
